{
  "name": "shubho-dev-11ty",
  "version": "1.0.0",
  "description": "Shubho.Dev 11ty site",
  "main": ".eleventy.js",
  "scripts": {
    "clean:dist": "del dist --force",
    "clean:tmp": "del src/_tmp",
    "clean": "run-p clean:dist clean:tmp",
    "prestart": "npm run clean",
    "start": "npm run dev",
    "eleventy:serve": "eleventy --watch --serve",
    "postcss:watch": "TAILWIND_MODE=watch NODE_ENV=development postcss src/styles/css/*.css --dir src/_tmp/css --config src/styles --watch",
    "postcss:build": "TAILWIND_MODE=build NODE_ENV=production postcss src/styles/css/*.css --dir src/_tmp/css --config src/styles",
    "watch:js": "gulp watch",
    "assets:watch": "run-p postcss:watch watch:js",
    "dev": "run-p assets:watch eleventy:serve"
  },
  "author": "Shubhojyoti Bhattacharya",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-cache-assets": "^2.2.1",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.2.6",
    "await-to-js": "^3.0.0",
    "axios": "^0.21.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "cssnano": "^5.0.5",
    "cssnano-preset-advanced": "^5.1.2",
    "dayjs": "^1.10.5",
    "del-cli": "^3.0.1",
    "dotenv": "^10.0.0",
    "gatsby-remark-prismjs": "^5.3.0",
    "gulp": "^4.0.2",
    "gulp-noop": "^1.0.1",
    "gulp-rev": "^9.0.0",
    "gulp-rev-replace": "^0.4.4",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "jsdom": "^16.6.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "parse-numeric-range": "^1.2.0",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "prismjs": "^1.23.0",
    "tailwindcss": "^2.1.2",
    "terser": "^5.7.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
